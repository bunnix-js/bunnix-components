{
  "name": "@bunnix/components",
  "version": "0.10.3",
  "description": "Bunnix components: a set of bunnix ready components for modern web apps.",
  "keywords": [
    "bunnix",
    "componentes",
    "reactive",
    "stateful",
    "web",
    "app"
  ],
  "homepage": "https://github.com/bunnix-js/bunnix-components#readme",
  "bugs": {
    "url": "https://github.com/bunnix-js/bunnix-components/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bunnix-js/bunnix-components.git"
  },
  "license": "ISC",
  "author": "@bunnix",
  "type": "module",
  "main": "src/index.mjs",
  "style": "src/core/core.css",
  "types": "@types/index.d.ts",
  "exports": {
    ".": {
      "types": "./@types/index.d.ts",
      "default": "./src/index.mjs"
    },
    "./styles": "./src/core/core.css",
    "./core.css": "./src/core/core.css",
    "./icons/*": "./src/icons/*"
  },
  "sideEffects": [
    "./src/core/*.css"
  ],
  "files": [
    "src",
    "@types"
  ],
  "scripts": {
    "test": "node --test",
    "sanitize:icons": "node scripts/sanitizeSvgs.mjs",
    "prebuild": "npm run sanitize:icons && node scripts/generateIconRegistry.mjs",
    "build:icons": "npm run sanitize:icons && node scripts/generateIconRegistry.mjs"
  },
  "peerDependencies": {
    "@bunnix/core": "^0.9.4"
  },
  "devDependencies": {
    "@bunnix/core": "^0.9.4"
  }
}
